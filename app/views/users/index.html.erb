<h1>Listing users</h1>

<table>
  <tr>
    <th>name</th>
    <th>password</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.name%></td>
      <td><%= user.password%></td>
    </tr>
  <% end %>
  <%= link_to 'New user', users_new_path%>
  <%= link_to("New user","/users/new")%> 
</table>
